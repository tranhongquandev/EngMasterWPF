<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:home="clr-namespace:EngMasterWPF.Views.HomeView"
    xmlns:mainView="clr-namespace:EngMasterWPF.Views"
    xmlns:viewmodel="clr-namespace:EngMasterWPF.ViewModel">
    <DataTemplate DataType="{x:Type viewmodel:AuthViewModel}">
        <mainView:AuthView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:MainViewModel}">
        <mainView:MainView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:HomeViewModel}">
        <home:HomeView />
    </DataTemplate>





</ResourceDictionary>