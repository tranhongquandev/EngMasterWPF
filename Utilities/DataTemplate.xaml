<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:view="clr-namespace:EngMasterWPF.Views"
    xmlns:viewmodel="clr-namespace:EngMasterWPF.ViewModel">
    <DataTemplate DataType="{x:Type viewmodel:HomeVM}">
        <view:HomePage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:StudentVM}">
        <view:StudentPage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:TeacherVM}">
        <view:TeacherPage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:CourseVM}">
        <view:CoursePage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:GradeVM}">
        <view:GradePage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:NotificationVM}">
        <view:NotificationPage />
    </DataTemplate>



</ResourceDictionary>