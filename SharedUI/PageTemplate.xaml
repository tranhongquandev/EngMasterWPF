<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:course="clr-namespace:EngMasterWPF.Views.CourseView"
    xmlns:grade="clr-namespace:EngMasterWPF.Views.GradeView"
    xmlns:home="clr-namespace:EngMasterWPF.Views.HomeView"
    xmlns:notification="clr-namespace:EngMasterWPF.Views.NotificationView"
    xmlns:student="clr-namespace:EngMasterWPF.Views.StudentView"
    xmlns:teacher="clr-namespace:EngMasterWPF.Views.TeacherView"
    xmlns:vm="clr-namespace:EngMasterWPF.ViewModel">

    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <home:HomeView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:StudentViewModel}">
        <student:StudentView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TeacherViewModel}">
        <teacher:TeacherView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CourseViewModel}">
        <course:CourseView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:GradeViewModel}">
        <grade:GradeView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:NotificationViewModel}">
        <notification:NotificationView />
    </DataTemplate>





</ResourceDictionary>